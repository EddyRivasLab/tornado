run:
----
cd ~/src/tornado/notebook/23-10_train/g6
qsub -N g6_train -cwd -b y -V -j y  -o /dev/null -pe impi 150 'mpirun -np 150 ~/src/tornado/src/grm-train --mpi --ml --margsavefile TORNADO_TestA_marginal.param --null TORNADO_TestA_null.param --countsavefile TORNADO_TestA_g6.counts ~/src/tornado/grammars/g6.grm ~/src/tornado/data/TORNADO/RNA2011_benchmark/TORNADO_TestA.sto TORNADO_TestA_g6.param > TORNADO_TestA_g6.mpitrain'
qsub -N g6_train -cwd -b y -V -j y  -o /dev/null -pe impi 150 'mpirun -np 150 ~/src/tornado/src/grm-train --mpi --ml --margsavefile TORNADO_TestB_marginal.param --null TORNADO_TestB_null.param --countsavefile TORNADO_TestB_g6.counts ~/src/tornado/grammars/g6.grm ~/src/tornado/data/TORNADO/RNA2011_benchmark/TORNADO_TestB.sto TORNADO_TestB_g6.param > TORNADO_TestB_g6.mpitrain'
qsub -N g6_train -cwd -b y -V -j y  -o /dev/null -pe impi 150 'mpirun -np 150 ~/src/tornado/src/grm-train --mpi --ml --margsavefile TORNADO_TestATestB_marginal.param --null TORNADO_TestATestB_null.param --countsavefile TORNADO_TestATestB_g6.counts ~/src/tornado/grammars/g6.grm ~/src/tornado/data/TORNADO/RNA2011_benchmark/TORNADO_TestATestB.sto TORNADO_TestATestB_g6.param > TORNADO_TestATestB_g6.mpitrain'
qsub -N g6_train -cwd -b y -V -j y  -o /dev/null -pe impi 150 'mpirun -np 150 ~/src/tornado/src/grm-train --mpi --ml --margsavefile tmark-Widmann_RNA2012-test_marginal.param --null tmark-Widmann_RNA2012-test_null.param --countsavefile tmark-Widmann_RNA2012-test_g6.counts ~/src/tornado/grammars/g6.grm ~/src/tornado/data/non_wc/TORNADO/tmark/tmark-Widmann_RNA2012-test.sto tmark-Widmann_RNA2012-test_g6.param > tmark-Widmann_RNA2012-test_g6.mpitrain'
